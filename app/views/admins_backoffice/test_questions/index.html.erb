<h1>Marque somente as questões escolhidas para a avaliação</h1>

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading"></div>
      <div class="panel-body">
        <div class="row">
          <div class="col-lg-6">

            <%= form_with(model: [ :admins_backoffice, @test], local: true) do |form| %>
              <%= %>
                <div class="form-group" id="questions">
                  <%= form.fields_for :questions do |question| %>
                    <%= render partial: "question_fields", locals: {f: question} %>
                  <% end %>
                </div> 
             <%= form.submit "Salvar", class:"btn btn-success  w-25", id:"btn"%>
           <% end %>
           
          </div>
          <!-- /.col-lg-6 (nested) -->
        </div>
        <!-- /.row (nested) -->
      </div>
      <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
  </div>
  <!-- /.col-lg-12 -->
</div>